{
  "name": "little-miss-robot/craft-searchable",
  "description": "A simple plugin to show which fields are searchable.",
  "type": "craft-plugin",
  "version": "1.0.0",
  "homepage": "https://littlemissrobot.com",
  "license": "mit",
  "keywords": [
    "cp",
    "control panel",
    "search",
    "searchable"
  ],
  "support": {
    "email": "craft@littlemissrobot.com",
    "issues": "https://github.com/little-miss-robot/craft-searchable/issues?state=open",
    "source": "https://github.com/little-miss-robot/craft-searchable",
    "docs": "https://github.com/little-miss-robot/craft-searchable",
    "rss": "https://github.com/little-miss-robot/craft-searchable/releases.atom"
  },
  "require": {
    "php": ">=8.0.2",
    "craftcms/cms": "^4.5.0"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "littlemissrobot\\craftsearchable\\": "src/"
    }
  },
  "extra": {
    "handle": "searchable",
    "name": "Searchable",
    "developer": "Little Miss Robot",
    "documentationUrl": "https://github.com/little-miss-robot/craft-searchable"
  },
  "scripts": {
    "check-cs": "ecs check --ansi",
    "fix-cs": "ecs check --ansi --fix",
    "phpstan": "phpstan --memory-limit=1G"
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.0.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}
